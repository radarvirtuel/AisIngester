generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_MYSQL")
}

model Station {
  id	Int @id
	city String
	country String
	typeBox String
	typeStation String
	email String
	createdAt DateTime @default(now())
	range Int @default(400)
	img String
	status Boolean
	lat Float
	lon Float
}

model Vessel {
  mmsi	Int @id
	shipName String?
	imo String?
	callSign String?
	shipType Int?
	owner String?
	updated DateTime @default(now())
}
